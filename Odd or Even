#include <stdio.h>

int main() {
    int numbers[] = {10, 23, 45, 66, 78, 91}; 
    int size = sizeof(numbers) / sizeof(numbers[0]);

    printf("Odd and Even numbers in the given set:\n");

    for (int i = 0; i < size; i++) {
        if (numbers[i] % 2 == 0)
            printf("%d is Even\n", numbers[i]);
        else
            printf("%d is Odd\n", numbers[i]);
    }

    return 0;
}
